#include <bits/stdc++.h>
using namespace std;
#define MOD 10e9 + 7

void solve() {
    int a, b, c;
    cin >> a >> b >> c;

    int sum = a + b + c;

    if (sum % 2 == 0) {
        sum /= 2;
        int x = sum - c;
        int y = sum - a;
        int z = sum - b;

        if (x >= 0 && y >= 0 && z >= 0) {
            cout << x << " " << y << " " << z <<"\n";
            return;
        }
            
    }
    cout << "Impossible\n";
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int T = 1;
    // cin >> T;
    while (T--) {
        solve();
    }

    return 0;
}
