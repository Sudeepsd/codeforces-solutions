#include <bits/stdc++.h>
using namespace std;
#define MOD 10e9 + 7

void solve() {
    int n; cin >> n;

    vector<int> a(101), b(101);
    for (int i = 0; i < n; ++i) {
        int x, y;
        cin >> x >> y;
        ++a[x];
        ++b[y];
    }

    int ans = 0;
    for (int i = 0; i < 101; ++i)
        ans += a[i] * b[i];
    cout << ans << "\n";
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int T = 1;
    // cin >> T;
    while (T--) {
        solve();
    }

    return 0;
}
