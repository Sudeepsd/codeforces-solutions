#include <bits/stdc++.h>
using namespace std;
#define MOD 10e9 + 7

void solve() {
    int n; cin >> n;

    vector<bool> seen(n + 1);
    int p; cin >> p;
    for (int i = 0; i < p; ++i) {
        int x; cin >> x;
        seen[x] = true;
    }

    cin >> p;
    for (int i = 0; i < p; ++i) {
        int x; cin >> x;
        seen[x] = true;
    }

    for (int i = 1; i <= n; ++i) {
        if (!seen[i]) {
            cout << "Oh, my keyboard!\n";
            return;
        }
    }
    cout << "I become the guy.\n";
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int T = 1;
    // cin >> T;
    while (T--) {
        solve();
    }

    return 0;
}
