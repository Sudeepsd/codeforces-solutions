#include <bits/stdc++.h>
using namespace std;
#define MOD 10e9 + 7

void solve() {
    int n, k;
    cin >> n >> k;

    string s;
    cin >> s;

    for (int i = 1; i <= k; ++i) {
        for (int j = 0; j < n - 1; ++j) {
            if (s[j] == 'B' && s[j+1] == 'G') {
                swap(s[j], s[j+1]);
                ++j;
            }
        }
    }

    cout << s << "\n";
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int T = 1;
    // cin >> T;
    while (T--) {
        solve();
    }

    return 0;
}
