#include <bits/stdc++.h>
using namespace std;
#define MOD 10e9 + 7

void solve() {
    int arr[4];
    cin >> arr[0] >> arr[1] >> arr[2] >> arr[3];

    sort(arr, arr + 4);
    cout << arr[3] - arr[0] << " "
        << arr[3] - arr[2] << " "
        << arr[3] - arr[1];
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int T = 1;
    // cin >> T;
    while (T--) {
        solve();
        cout << "\n";
    }

    return 0;
}
