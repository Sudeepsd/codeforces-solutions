#include <bits/stdc++.h>
using namespace std;
#define MOD 10e9 + 7

void solve() {
    string a, b, c;
    cin >> a >> b >> c;

    vector<int> freq(26, 0);
    for (char ch : a)
        ++freq[ch - 'A'];
    for (char ch : b)
        ++freq[ch - 'A'];
    for (char ch : c)
        --freq[ch - 'A'];

    for (int i = 0; i < 26; ++i) {
        if (freq[i] != 0) {
            cout << "NO";
            return;
        }
    }
    cout << "YES";
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int T = 1;
    // cin >> T;
    while (T--) {
        solve();
        cout << "\n";
    }

    return 0;
}
