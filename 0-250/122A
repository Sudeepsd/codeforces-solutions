#include <bits/stdc++.h>
using namespace std;

vector<int> luckyNumbers = {4, 7, 44, 47, 74, 77, 444, 447, 474, 477, 744, 747, 774, 777};

void solve() {
    int n; cin >> n;

    for (int x : luckyNumbers)
        if (n % x == 0) {
            cout << "YES\n";
            return;
        }
    cout << "NO\n";
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int T = 1;
    // cin >> T;
    while (T--) {
        solve();
    }

    return 0;
}
