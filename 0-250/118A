#include <bits/stdc++.h>
using namespace std;

void solve() {
    string s;
    cin >> s;

    const string vowels = "AEIOUYaeioyu";

    for (char ch : s) {
        if (vowels.find(ch) != string::npos)
            continue;
        
        cout << "." << (char)tolower(ch);
    }
    cout << "\n";
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int T = 1;
    // cin >> T;
    while (T--) {
        solve();
    }

    return 0;
}
