#include <bits/stdc++.h>
using namespace std;
#define MOD 10e9 + 7

void solve() {
    int a, b, c, d;
    cin >> a >> b >> c >> d;

    unordered_set<int> shoes = {a, b, c, d};

    cout << 4 - shoes.size() << "\n";
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int T = 1;
    // cin >> T;
    while (T--) {
        solve();
    }

    return 0;
}
