#include <bits/stdc++.h>
using namespace std;
#define MOD 10e9 + 7

void solve() {
    int n; cin >> n;
    int arr[n+1];

    for (int i = 1; i <= n; ++i)
        cin >> arr[i];
    
    int r = -1;
    for (int i = 1; i <= n; ++i) {
        if (arr[i] != i) {
            r = i;
            break;
        }
        cout << i << " ";
    }

    if (r == -1) {
        cout << "\n";
        return;
    }

    int l = -1;
    for (int i = r; i <= n; ++i) {
        if (arr[i] == r) {
            l = i;
            break;
        }
    }

    for (int i = l; i >= r; --i) {
        cout << arr[i] << " ";
    }

    for (int i = l + 1; i <= n; ++i) {
        cout << arr[i] << " ";
    }
    cout << "\n";
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int T = 1;
    cin >> T;
    while (T--) {
        solve();
    }

    return 0;
}
