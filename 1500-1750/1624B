#include <bits/stdc++.h>
using namespace std;
#define MOD 10e9 + 7

void solve() {
    int a, b, c;
    cin >> a >> b >> c;

    if (2*b - c > 0 && (2*b - c) % a == 0) {
        cout << "YES\n";
        return;
    }

    if (2*b - a > 0 && (2*b - a) % c == 0) {
        cout << "YES\n";
        return;
    }

    if ((a + c) > 0 && (a + c) % (2*b) == 0) {
        cout << "YES\n";
        return;
    }
    cout << "NO\n";
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int T = 1;
    cin >> T;
    while (T--) {
        solve();
    }

    return 0;
}
