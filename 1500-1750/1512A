#include <bits/stdc++.h>
using namespace std;

void solve() {
    int n; cin >> n;
    int arr[n];

    for (int i = 0; i < n; ++i)
        cin >> arr[i];

    int notSus;
    if (arr[0] == arr[1] || arr[0] == arr[2])
        notSus = arr[0];
    else {
        cout << 1 << "\n";
        return;
    }

    for (int i = 1; i < n; ++i) {
        if (arr[i] != notSus)
            cout << i + 1 << "\n";
    }
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int T = 1;
    cin >> T;
    while (T--) {
        solve();
    }

    return 0;
}
