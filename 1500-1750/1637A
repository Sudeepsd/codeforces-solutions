#include <bits/stdc++.h>
using namespace std;
#define MOD 10e9 + 7

void solve() {
    int n; cin >> n;
    int arr[n];
    
    for (int i = 0; i < n; ++i)
        cin >> arr[i];

    if (is_sorted(arr, arr + n))
        cout << "NO\n";
    else
        cout << "YES\n";
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int T = 1;
    cin >> T;
    while (T--) {
        solve();
    }

    return 0;
}
