#include <bits/stdc++.h>
using namespace std;

void solve() {
    int n; cin >> n;
    string s = to_string(n);

    int ans = 9 * (s.length() - 1);

    int l = 0;
    for (int i = 0; i < s.length(); ++i) {
        if (s[i] != s[0]) {
            l = i;
            break;
        }
    }

    if (l == 0 || s[0] < s[l])
        cout << ans + s[0] - '0';
    else
        cout << ans + s[0] - '0' - 1;
    cout << "\n";
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int T = 1;
    cin >> T;
    while (T--) {
        solve();
    }

    return 0;
}
