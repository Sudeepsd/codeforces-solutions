#include <bits/stdc++.h>
using namespace std;

void solve() {
    string s;
    cin >> s;

    char ch; cin >> ch;

    for (int i = 0; i < s.length(); ++i) {
        if (s[i] == ch && i % 2 == 0) {
            cout << "YES\n";
            return;
        }
    }
    cout << "NO\n";
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int T = 1;
    cin >> T;
    while (T--) {
        solve();
    }

    return 0;
}
