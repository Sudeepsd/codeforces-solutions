#include <bits/stdc++.h>
using namespace std;
#define MOD 10e9 + 7

void solve() {
    int n; cin >> n;

    if (n % 7 == 0) {
        cout << n << "\n";
        return;
    }

    n = (n / 10) * 10;
    for (int i = 0; i < 10; ++i) {
        if ((n + i) % 7 == 0) {
            cout << (n + i) << "\n";
            return;
        }
    }
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int T = 1;
    cin >> T;
    while (T--) {
        solve();
    }

    return 0;
}
