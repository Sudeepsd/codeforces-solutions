#include <bits/stdc++.h>
using namespace std;
#define MOD 10e9 + 7

void solve() {
    int n; cin >> n;
    vector<bool> arr(n + 1, false);

    for (int i = 0; i < n; ++i) {
        int x; cin >> x;
        while (x != 0 && (x > n || arr[x]))
            x >>= 1;
        arr[x] = true;
    }
    
    if (arr[0])
        cout << "NO\n";
    else
        cout << "YES\n";
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int T = 1;
    cin >> T;
    while (T--) {
        solve();
    }

    return 0;
}
