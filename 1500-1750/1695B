#include <bits/stdc++.h>
using namespace std;

void solve() {
    int n; cin >> n;

    int arr[n];
    for (int i = 0; i < n; ++i)
        cin >> arr[i];

    for (int i = 0; i < n; ++i) {
        if (arr[i] == 0) {
            if (i % 2 == 0)
                cout << "Mike\n";
            else
                cout << "Joe\n";
            return;
        }
    }
    
    if (n & 1) {
        cout << "Mike\n";
    } else {
        int mini = INT_MAX;
        for (int i = 0; i < n; ++i) {
            mini = min(mini, arr[i]);
        }

        for (int i = 0; i < n; ++i) {
            if (arr[i] == mini) {
                if (i % 2 == 0) {
                    cout << "Joe\n";
                } else {
                    cout << "Mike\n";
                }
                return;
            }
        }
    }
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int T = 1;
    cin >> T;
    while (T--) {
        solve();
    }

    return 0;
}
