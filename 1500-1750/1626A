#include <bits/stdc++.h>
using namespace std;
#define MOD 10e9 + 7

void solve() {
    string s;
    cin >> s;

    sort(s.begin(), s.end());

    cout << s << "\n";
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int T = 1;
    cin >> T;
    while (T--) {
        solve();
    }

    return 0;
}
