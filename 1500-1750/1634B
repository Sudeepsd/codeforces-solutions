#include <bits/stdc++.h>
using namespace std;
#define MOD 10e9 + 7

void solve() {
    long long n, x, y;
    cin >> n >> x >> y;

    for (int i = 0; i < n; ++i) {
        int a; cin >> a;
        x += (a & 1);
    }

    if ((x & 1) == (y & 1))
        cout << "Alice\n";
    else
        cout << "Bob\n";
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int T = 1;
    cin >> T;
    while (T--) {
        solve();
    }

    return 0;
}
