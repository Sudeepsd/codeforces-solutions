#include <bits/stdc++.h>
using namespace std;

void solve() {
    int n, m;
    cin >> n >> m;

    int total = 0;
    for (int i = 0; i < n; ++i) {
        int x; cin >> x;
        total += x;
    }
    if (total > m)
        cout << total - m << "\n";
    else
        cout << "0\n";
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int T = 1;
    cin >> T;
    while (T--) {
        solve();
    }

    return 0;
}
