#include <bits/stdc++.h>
using namespace std;

void solve() {
    string s;
    cin >> s;

    int a = 0;
    for (int i = 0; i < 3; ++i)
        a += s[i] - '0';

    int b = 0;
    for (int i = 3; i < 6; ++i)
        b += s[i] - '0';

    if (a == b)
        cout << "YES\n";
    else
        cout << "NO\n";
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int T = 1;
    cin >> T;
    while (T--) {
        solve();
    }

    return 0;
}
