#include <bits/stdc++.h>
using namespace std;

void solve() {
    int n; cin >> n;
    unordered_set<int> uset;

    for (int i = 1; i * i <= n; ++i)
        uset.insert(i * i);
    for (int i = 1; i * i * i <= n; ++i)
        uset.insert(i * i * i);

    cout << uset.size() << "\n";
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int T = 1;
    cin >> T;
    while (T--) {
        solve();
    }

    return 0;
}
