#include <bits/stdc++.h>
using namespace std;

void solve() {
    string s;
    cin >> s;

    int arr[2] = {0};
    for (char ch : s) {
        ++arr[ch - '0'];
    }

    int k = min(arr[0], arr[1]);
    if (k & 1)
        cout << "DA\n";
    else
        cout << "NET\n";
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int T = 1;
    cin >> T;
    while (T--) {
        solve();
    }

    return 0;
}
