#include <bits/stdc++.h>
using namespace std;

void solve() {
    long long a, b, c;
    cin >> a >> b >> c;

    if (a < c)
        cout << "1 ";
    else
        cout << "-1 ";

    if (a * b > c)
        cout << b << "\n";
    else
        cout << "-1\n";
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int T = 1;
    cin >> T;
    while (T--) {
        solve();
    }

    return 0;
}
