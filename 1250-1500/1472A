#include <bits/stdc++.h>
using namespace std;

void solve() {
    int w, h, k;
    cin >> w >> h >> k;

    long long cnt = 1;
    while (w % 2 == 0 || h % 2 == 0) {
        cnt <<= 1;
        if (w % 2 == 0)
            w /= 2;
        else
            h /= 2;
    }

    if (cnt >= k)
        cout << "YES\n";
    else
        cout << "NO\n";
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int T = 1;
    cin >> T;
    while (T--) {
        solve();
    }

    return 0;
}
