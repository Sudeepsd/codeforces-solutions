#include <bits/stdc++.h>
using namespace std;
#define MOD 10e9 + 7

void solve() {
    int a, b;
    cin >> a >> b;

    if (a > b)
        swap(a, b);

    cout << a << " " << (b - a) / 2 << "\n";
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int T = 1;
    // cin >> T;
    while (T--) {
        solve();
    }

    return 0;
}
