#include <bits/stdc++.h>
using namespace std;

void solve() {
    int n, t;
    cin >> n >> t;

    int time = 240 - t;
    int i;
    for (i = 1; i <= n; ++i) {
        time -= 5 * i;
        if (time < 0)
            break;
    }

    cout << i - 1 << "\n";
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int T = 1;
    // cin >> T;
    while (T--) {
        solve();
    }

    return 0;
}
