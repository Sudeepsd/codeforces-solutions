#include <bits/stdc++.h>
using namespace std;

void solve() {
    int m, n;
    cin >> m >> n;

    bool color = false;
    for (int i = 0; i < m; ++i) {
        for (int j = 0; j < n; ++j) {
            char ch; cin >> ch;
            if (ch == 'C' || ch == 'M' || ch == 'Y')
                color = true;
        }
    }

    if (color)
        cout << "#Color\n";
    else
        cout << "#Black&White\n";
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int T = 1;
    // cin >> T;
    while (T--) {
        solve();
    }

    return 0;
}
