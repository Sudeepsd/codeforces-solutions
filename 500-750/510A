#include <bits/stdc++.h>
using namespace std;
#define MOD 10e9 + 7

void solve() {
    int m, n;
    cin >> m >> n;

    bool last = true;
    for (int i = 0; i < m; ++i) {
        if (i % 2 == 0) {
            for (int j = 0; j < n; ++j)
                cout << "#";
        } else {
            if (!last)
                cout << "#";
            for (int j = 1; j < n; ++j)
                cout << ".";
            if (last)
                cout << "#";
            last = !last;
        }
        cout << "\n";
    }
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int T = 1;
    // cin >> T;
    while (T--) {
        solve();
    }

    return 0;
}
