#include <bits/stdc++.h>
using namespace std;

void solve() {
    int n; cin >> n;

    int cnt = 0;
    for (int x : {100, 20, 10, 5, 1}) {
        cnt += n / x;
        n %= x;
    }
    cout << cnt << "\n";
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int T = 1;
    // cin >> T;
    while (T--) {
        solve();
    }

    return 0;
}
